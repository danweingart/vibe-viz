{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/vibestr/snapshot",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/admin/cleanup-cache",
      "schedule": "0 */6 * * *"
    }
  ]
}
